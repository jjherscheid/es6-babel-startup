{
    "name": "es-babel-startup",
    "version": "0.0.1",
    "description": "ES6 Babel Startup project",
    "author": "Johan Herscheid",
    "dependencies": {},
    "devDependencies": {
        "babel-cli": "^6.9.0",
        "babel-preset-es2015": "^6.9.0"
    },
    "scripts": {
        "build": "babel src -d lib --source-maps --watch"
    },
    "babel": {
        "presets": [
            "es2015"
        ]
    },
    "eslintConfig": {
        "parserOptions": {
            "ecmaVersion": 6,
            "sourceType": "module"
        }
    },
    "jshintConfig": {
        "esversion": 6
    }
}
